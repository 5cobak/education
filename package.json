  "scripts": {
    "start": "webpack serve --env mode=development port=3000",
    "build": "webpack --env mode=production",
    "updateL": "babel -f .babelrc src/**/*.tsx",
    "unit": "jest --config ./config/jest/jest.config.ts",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "loki:test": "npx loki test",
    "loki:approve": "npx loki approve && npm run loki:test",
    "loki:report": "reg-cli .loki/current .loki/reference .loki/reg-diff / -R .loki/report.html",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "storybook:build": "build-storybook -c ./config/storybook"
  },
